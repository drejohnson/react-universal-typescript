{
  "name": "react-universal-typescript",
  "version": "0.0.1",
  "description": "React starter for building universal apps with Typescript, Webpack 2, Apollo",
  "scripts": {
    "analyze": "webpack --config webpack/webpack.client.ts --profile --json > stats.json",
    "lint": "eslint . --ext js,gql,graphql",
    "persistgraphql": "persistgraphql ui --add_typename",
    "generate-graphql-types": "apollo-codegen generate ui/graphql/**/*.graphql --schema schema.json --target ts --output ui/schema.ts",
    "generate-graphql-documents": "cd ./ui/graphql; graphql-document-collector **/*.graphql > documents.json",
    "prebuild": "rimraf build/*",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress --config webpack/webpack.client.ts",
    "build:server": "webpack --progress --config webpack/webpack.server.ts",
    "start": "node build/server.js",
    "prestart-dev": "npm run build",
    "start-dev": "nodemon build/server.js"
  },
  "dependencies": {
    "apollo-client": "^0.10.1",
    "body-parser": "^1.17.0",
    "compression": "1.6.2",
    "dotenv": "^4.0.0",
    "express": "4.15.0",
    "graphql-anywhere": "^2.2.0",
    "graphql-tag": "^1.3.1",
    "history": "^4.5.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.0.2",
    "morgan": "1.8.1",
    "persistgraphql": "^0.3.0",
    "picturefill": "^3.0.2",
    "react": "^15.4.2",
    "react-apollo": "^0.13.2",
    "react-async-component": "^0.2.2",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-helmet": "^4.0.0",
    "react-redux": "5.0.3",
    "react-router-dom": "^4.0.0-beta.6",
    "recompose": "^0.22.0",
    "serialize-javascript": "^1.3.0",
    "styled-components": "2.0.0-5",
    "subscriptions-transport-ws": "^0.5.3",
    "video.js": "^6.0.0-RC.2",
    "videojs-contrib-ads": "^4.2.4",
    "webfontloader": "^1.6.27"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.34",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/history": "^4.5.0",
    "@types/immutable": "^3.8.7",
    "@types/isomorphic-fetch": "0.0.33",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "@types/react-helmet": "^0.0.28",
    "@types/react-redux": "^4.4.36",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "^3.0.37",
    "@types/redux-devtools-dock-monitor": "^1.1.30",
    "@types/redux-devtools-log-monitor": "^1.0.30",
    "@types/systemjs": "^0.20.1",
    "@types/videojs": "^5.16.1",
    "@types/webpack": "^2.2.7",
    "apollo-codegen": "^0.10.8",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "^3.0.8",
    "babel-eslint": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.16.1",
    "eslint-plugin-graphql": "^0.6.1",
    "file-loader": "0.10.1",
    "graphql-document-collector": "^0.1.1",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "rimraf": "2.6.1",
    "source-map-loader": "^0.1.6",
    "source-map-support": "^0.4.11",
    "sw-precache-webpack-plugin": "^0.9.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "tslint-react": "^2.4.0",
    "typescript": "2.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^3.0.0",
    "webpack-node-externals": "1.5.4"
  }
}
