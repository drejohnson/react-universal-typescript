{
  "name": "react-universal-typescript",
  "version": "0.0.1",
  "description": "React starter for building universal apps with Typescript, Webpack 2, Apollo",
  "scripts": {
    "analyze": "webpack --config webpack/webpack.client.ts --profile --json > stats.json",
    "prebuild": "rimraf build/*",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress --config webpack/webpack.client.ts",
    "build:server": "webpack --progress --config webpack/webpack.server.ts",
    "start": "node build/server.js",
    "prestart-dev": "npm run build",
    "start-dev": "nodemon build/server.js"
  },
  "dependencies": {
    "apollo-client": "^0.8.2",
    "body-parser": "^1.16.0",
    "compression": "1.6.2",
    "express": "4.14.1",
    "graphql-anywhere": "^2.2.0",
    "graphql-tag": "^1.2.4",
    "history": "^4.5.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "1.7.0",
    "react": "^15.4.2",
    "react-apollo": "^0.10.0",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "5.0.2",
    "react-router": "next",
    "react-router-dom": "next",
    "serialize-javascript": "^1.3.0",
    "styled-components": "2.0.0-3"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/history": "^4.5.0",
    "@types/immutable": "^3.8.7",
    "@types/isomorphic-fetch": "^0.0.31",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.6",
    "@types/react-dom": "^0.14.22",
    "@types/react-helmet": "^0.0.28",
    "@types/react-redux": "^4.4.36",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "^3.0.37",
    "@types/redux-devtools-dock-monitor": "^1.1.30",
    "@types/redux-devtools-log-monitor": "^1.0.30",
    "@types/webpack": "^2.2.4",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "^3.0.3",
    "compression-webpack-plugin": "^0.3.2",
    "file-loader": "0.10.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "rimraf": "2.5.4",
    "source-map-loader": "^0.1.6",
    "source-map-support": "^0.4.11",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.3.0",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^2.6.1",
    "webpack-node-externals": "1.5.4"
  }
}
